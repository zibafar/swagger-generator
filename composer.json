{
    "name": "zibafar/swagger-generator",
    "description": "This is my package swagger-generator",
    "keywords": [
        "zibafar",
        "laravel",
        "swagger-generator"
    ],
    "homepage": "https://github.com/zibafar/swagger-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Zibafar",
            "email": "sadjadzibafar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "barryvdh/reflection-docblock": "^2.0.6",
        "doctrine/dbal": "^2.6 || ^3",
        "composer/class-map-generator": "^1.0",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Zibafar\\SwaggerGenerator\\": "src/",
            "Zibafar\\SwaggerGenerator\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zibafar\\SwaggerGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zibafar\\SwaggerGenerator\\SwaggerGeneratorServiceProvider"
            ],
            "aliases": {
                "SwaggerGenerator": "Zibafar\\SwaggerGenerator\\Facades\\SwaggerGenerator"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0"
}
